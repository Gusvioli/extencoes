{
  "manifest_version": 3,
  "name": "SPID Delivery",
  "version": "1.0.0",
  "description": "Uma extens√£o para verificar se existe uma entrega SPID no delivery.",
  "author": "Gustavo Vieira",
  "icons": {
    "16": "delivery-bike_9561839.png",
    "48": "delivery-bike_9561839.png",
    "128": "delivery-bike_9561839.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "notifications",
    "tts",
    "alarms"
  ],
  "host_permissions": [
    "https://janis.in/*"
  ],
  "background": {
    "service_worker": "src/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://janis.in/*"],
      "js": ["src/content.js"]
    }
  ],
  "action": {
    "default_popup": "src/popup/popup.html"
  }
}